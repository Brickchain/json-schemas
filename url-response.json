{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$id": "https://developer.brickchain.com/schemas/url-response.json",
    "title": "URL Response",
    "description": "This is the URL Response schema, used for sending the Integrity app an URL as a response.",
    "type": "object",
    "allOf": [ { "$ref": "https://developer.brickchain.com/schemas/base.json" } ],
    "properties": {
        "url": {
            "type": "string",
            "format": "uri",
            "description": "The URL."
        }
    },
    "required": [
        "url"
    ]
}
